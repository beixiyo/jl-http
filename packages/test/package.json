{
  "name": "test-page",
  "type": "module",
  "version": "0.0.0",
  "private": true,
  "scripts": {
    "dev": "node ./scripts/autoWriteStyle.cjs && vite",
    "build": "vite build",
    "build-and-lint": "tsc -b && vite build",
    "preview": "vite preview",
    "test": "vitest run",
    "test:watch": "vitest",
    "test:ui": "vitest --ui",
    "test:coverage": "vitest run --coverage",
    "test:coverage:watch": "vitest --coverage",
    "test:report": "vitest run --coverage && node ./scripts/test-report.js",
    "test:unit": "vitest run test/core test/tools",
    "test:integration": "vitest run test/integration",
    "test:cli": "vitest run test/cli",
    "test:all": "pnpm test:unit && pnpm test:integration && pnpm test:cli",
    "test:verify": "node run-all-tests.js --categories",
    "test:full": "node run-all-tests.js --full",
    "lint": "eslint ./src --fix"
  },
  "dependencies": {
    "@jl-org/http": "workspace:*",
    "@jl-org/tool": "^2.0.6",
    "@jl-org/vite-auto-route": "^1.0.6",
    "class-variance-authority": "^0.7.1",
    "clsx": "^2.1.1",
    "framer-motion": "^11.5.6",
    "lucide-react": "^0.514.0",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "react-router-dom": "^6.26.1",
    "tailwind-merge": "^2.5.4",
    "turndown": "^7.2.0"
  },
  "devDependencies": {
    "@jl-org/js-to-style": "^0.0.2",
    "@jl-org/ts-tool": "^0.0.8",
    "@types/node": "^22.5.2",
    "@types/react": "^18.3.5",
    "@types/react-dom": "^18.3.0",
    "@types/turndown": "^5.0.5",
    "@unocss/preset-legacy-compat": "^66.1.2",
    "@unocss/preset-wind3": "^66.1.2",
    "@unocss/reset": "^66.1.2",
    "@vitejs/plugin-react": "^4.3.4",
    "@vitest/coverage-v8": "^2.1.8",
    "@vitest/ui": "^3.2.4",
    "code-inspector-plugin": "^0.20.3",
    "jsdom": "^25.0.1",
    "postcss": "^8.4.43",
    "postcss-preset-env": "^10.1.2",
    "typescript": "^5.8.3",
    "unocss": "^66.1.2",
    "unplugin-auto-import": "^0.18.2",
    "vite": "^6.0.4",
    "vite-plugin-env-parse": "^1.0.15",
    "vitest": "^3.2.4"
  }
